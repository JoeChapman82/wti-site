<div class="wti-card wti-card-full">
    <div class="wti-card-header">
        {{ icons.case('existing-cases-icon') }}
        {{ cardHeading }}
    </div>
    <div class="wti-card-content">
        <table id="wtiTable" class="wti-table">
            <thead>
                <tr class="wti-sortable-header">
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="0"><div class="wti-table-th-text">Case No</div><div class="wti-table-icons"><div id="wtiTableSortUp0" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown0" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="1"><div class="wti-table-th-text">Group</div><div class="wti-table-icons"><div id="wtiTableSortUp1" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown1" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-sortable-table" data-column="2"><div class="wti-table-th-text">Identity</div><div class="wti-table-icons"><div id="wtiTableSortUp2" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown2" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="3"><div class="wti-table-th-text">Date added</div><div class="wti-table-icons"><div id="wtiTableSortUp3" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown3" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-sortable-table" data-column="4"><div class="wti-table-th-text">Class</div><div class="wti-table-icons"><div id="wtiTableSortUp4" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown4" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-sortable-table" data-column="5"><div class="wti-table-th-text">Place of rescue</div><div class="wti-table-icons"><div id="wtiTableSortUp5" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown5" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="6"><div class="wti-table-th-text">Zone</div><div class="wti-table-icons"><div id="wtiTableSortUp6" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown6" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="7"><div class="wti-table-th-text">Created by</div><div class="wti-table-icons"><div id="wtiTableSortUp7" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown7" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                    <th class="wti-mobile-hidden wti-sortable-table" data-column="8"><div class="wti-table-th-text">Last updated by</div><div class="wti-table-icons"><div id="wtiTableSortUp8" class="wti-table-icon wti-table-icon-left">{{ icons.upArrowTable('wti-table-svg') }}</div><div id="wtiTableSortDown8" class="wti-table-icon">{{ icons.downArrowTable('wti-table-svg') }}</div></div></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="caseNumber" id="caseNumber">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="groupName" id="groupName">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="idenityName" id="idenityName">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><input class="wti-table-search" type="date" name="dateAdded" id="dateAdded"></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="class" id="class">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="placeOfRescue" id="placeOfRescue">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="zoneName" id="zoneName">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="createdBy" id="addedBy">{{ icons.search('wti-table-serch-icon') }}</div></td>
                    <td class="wti-table-search-cell"><div class="wti-table-search-wrapper"><input class="wti-table-search" type="text" name="lastUpdatedBy" id="zone">{{ icons.search('wti-table-serch-icon') }}</div></td>
                </tr>
                {% for record in records %}
                    <tr class="wti-clickable-row" data-link="wtiLink{{ loop.index }}">
                        <td class="wti-mobile-hidden">{{ record.caseNumber }}</td>
                        <td class="wti-mobile-hidden">{{ record.groupName }}</td>
                        <td>{{ record.identityName }}
                            <a id="wtiLink{{ loop.index }}" href="/admin/existing-case/{{ record._id }}"></a>
                        </td>
                        <td class="wti-mobile-hidden">{{ record.dateAdded | asDayMonthYearTime }}</td>
                        <td class="">{{ record.class }}</td>
                        <td>{{ record.placeOfRescue }}</td>
                        <td class="wti-mobile-hidden">{{ record.zoneName }}</td>
                        <td class="wti-mobile-hidden">{{ record.createdBy }}</td>
                        <td class="wti-mobile-hidden">{{ record.lastUpdatedBy }}</td>
                    </tr>
                {% endfor %}
            </tbody>
            <tfoot>
                <tr>
                    <th class="wti-mobile-hidden">Case number</th>
                    <th class="wti-mobile-hidden">Group</th>
                    <th>Identity</th>
                    <th class="wti-mobile-hidden">Date added</th>
                    <th>Class</th>
                    <th>Place of rescue</th>
                    <th class="wti-mobile-hidden">Zone</th>
                    <th class="wti-mobile-hidden">Created by</th>
                    <th class="wti-mobile-hidden">Last updated by</th>

                </tr>
            </tfoot>
        </table>
    </div>
    <div class="wti-card-footer">
        Last update: {{ lastupdated | asDayMonthYearTime }}
    </div>
</div>
